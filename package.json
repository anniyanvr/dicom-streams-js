{
  "name": "@exini/dicom-streams-js",
  "version": "0.0.34",
  "description": "Streaming parsing and processing of DICOM data",
  "main": "dist/node/index.js",
  "browser": "dist/web/index.js",
  "types": "dist/node/index.d.ts",
  "scripts": {
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu mocha -r ts-node/register test/**/*-test.ts",
    "test-one": "cross-env NODE_ICU_DATA=node_modules/full-icu mocha 'test/parser-test.js'",
    "test-coverage": "cross-env NODE_ICU_DATA=node_modules/full-icu ./node_modules/istanbul/lib/cli.js cover -x \"**/tag.js\" -x \"**/tag-to-vr.js\" ./node_modules/mocha/bin/_mocha -- -R spec test/*-test.js",
    "build": "shx rm -rf dist; webpack --config webpack.config.js",
    "publish-to-npm": "npm run build; shx cp package.json LICENSE README.md dist; npm publish dist"
  },
  "keywords": [
    "dicom",
    "streams"
  ],
  "author": "karl.sostrand@exini.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/exini/dicom-streams-js"
  },
  "bugs": {
    "url": "https://github.com/exini/dicom-streams-js/issues"
  },
  "dependencies": {
    "readable-stream": "^3.1.1",
    "multipipe": "^3.0.0",
    "js-joda": "^1.10.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "typescript": "^3.5.2",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "@types/node": "*",
    "@types/readable-stream": "*",
    "@types/uuid": "*",
    "@types/mocha": "*",
    "full-icu": "^1.2.1",
    "shx": "^0.2.2",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "istanbul": "^0.4.5",
    "coveralls": "^3.0.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-merge": "^4.2.1"
  }
}
